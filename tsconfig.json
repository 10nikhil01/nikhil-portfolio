{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next",
      },
    ],
    "paths": {
      "@/*": ["./src/*"],
      "@/homepage": ["./src/components/homepage/components"],
      // "@/organicpages/*": ["./src/components/organicpages/components/*"],
      // "@/organicpages": ["./src/components/organicpages/components"],
      // "@/discover/*": ["./src/components/pages/DiscoverGenerations/*"],
      // "@/discover": ["./src/components/pages/DiscoverGenerations"],
    },
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts"],
  "exclude": ["node_modules"],
}
